modules = ["python-3.11"]

[nix]
channel = "stable-24_05"
packages = ["SDL2", "SDL2_image", "SDL2_mixer", "SDL2_ttf", "freetype", "glibcLocales", "gumbo", "harfbuzz", "jbig2dec", "jdk", "libcxx", "libjpeg_turbo", "libyaml", "mtdev", "mupdf", "openjpeg", "pkg-config", "swig", "xcbuild"]

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "APK Build Monitor"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "AGI Deployment Demo"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "AGI Trainer Demo"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "AGI Intelligence Router"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Verbal Conversation Training"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "AGI Learning Backup"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "AGI Real-Time Communication"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Main AGI App"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Document Learning"

[[workflows.workflow]]
name = "APK Build Monitor"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 autonomous_agi_monitor.py"

[[workflows.workflow]]
name = "AGI Deployment Demo"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run deployment_demo.py --server.port 5002"
waitForPort = 5002

[[workflows.workflow]]
name = "AGI Trainer Demo"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run agi_trainer_demo.py --server.port 5003"
waitForPort = 5003

[[workflows.workflow]]
name = "AGI Intelligence Router"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run agi_intelligent_ai_router.py --server.port 5005"
waitForPort = 5005

[[workflows.workflow]]
name = "Verbal Conversation Training"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run agi_verbal_conversation_trainer.py --server.port 5006"
waitForPort = 5006

[[workflows.workflow]]
name = "AGI Learning Backup"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 agi_learning_backup_system.py"

[[workflows.workflow]]
name = "AGI Real-Time Communication"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 agi_realtime_communication.py"

[[workflows.workflow]]
name = "Main AGI App"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app_optimized.py --server.port 5000"
waitForPort = 5000

[[workflows.workflow]]
name = "Document Learning"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run debug_app.py --server.port 8502"
waitForPort = 8502

